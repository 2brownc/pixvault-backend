{
  "name": "pixvault-backend",
  "version": "1.0.0",
  "description": "Backend for a reliable and comprehensive stock image website that provides collection of high-quality images for various creative projects.",
  "main": "index.js",
  "scripts": {
    "start": "npx nodemon src/index.ts",
    "build": "npx tsc",
    "preview": "node dist/index.js",
    "format": "npx @biomejs/biome format --write ./src",
    "lint": "npx @biomejs/biome lint ./src",
    "lint:fix": "npx @biomejs/biome lint --apply ./src"
  },
  "keywords": [
    "stock",
    "images",
    "images",
    "stock",
    "jpg",
    "png",
    "wallpaper"
  ],
  "author": "Brown Chinta",
  "license": "AGPL-3.0-or-later",
  "dependencies": {
    "@types/express": "^4.17.21",
    "axios": "^1.6.5",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-oauth2-jwt-bearer": "^1.6.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.1",
    "@flydotio/dockerfile": "^0.5.0",
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.5",
    "dotenv": "^16.3.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
