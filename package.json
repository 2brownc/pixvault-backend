{
  "name": "pixvault-backend",
  "version": "1.0.0",
  "description": "Backend for a reliable and comprehensive stock image website that provides collection of high-quality images for various creative projects.",
  "main": "index.js",
  "scripts": {
    "start:dev": "npx tsc --watch & nodemon dist/index.js",
    "build": "npx tsc",
    "lint": "npx eslint . --ext .ts",
    "format": "npx prettier --write \"src/**/*.ts\"",
    "start": "node dist/index.js",
    "build:prod": "npx tsc --build tsconfig.prod.json"
  },
  "keywords": [
    "stock",
    "images",
    "images",
    "stock",
    "jpg",
    "png",
    "wallpaper"
  ],
  "author": "Brown Chinta",
  "license": "AGPL-3.0-or-later",
  "dependencies": {
    "@types/express": "^4.17.21",
    "axios": "^1.6.5",
    "express": "^4.18.2",
    "mongoose": "^8.1.0"
  },
  "devDependencies": {
    "dotenv": "^16.3.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3"
  }
}
